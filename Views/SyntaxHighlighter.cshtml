@{
    Style.Require("TheMonarch.prettify-tomorrow-night").AtHead();     
    Script.Require("TheMonarch.prettify").AtHead();
}
@using (Script.Foot()) {
<script type="text/javascript">
    $(document).ready(function () {
        $('pre code').parent().addClass('prettyprint');
        prettyPrint();
    }); 
</script>
}